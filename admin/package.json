{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cp env.development .env && next dev -p 3000",
    "dev:local": "cp env.development .env && NEXT_PUBLIC_API_URL=http://localhost:3002/api next dev -p 3000",
    "dev:env": "cp env.development .env",
    "prod:env": "cp env.production .env",
    "build": "next build",
    "build:dev": "cp env.development .env && next build",
    "build:prod": "cp env.production .env && NODE_ENV=production next build",
    "start": "cp env.production .env && next start -p 3000",
    "start:dev": "cp env.development .env && next start -p 3000",
    "start:prod": "cp env.production .env && NODE_ENV=production next start -p 3000",
    "lint": "eslint",
    "pm2:start": "cp env.production .env && pm2 start ecosystem.config.js --only urbanesta-admin-frontend",
    "pm2:start:dev": "cp env.development .env && pm2 start ecosystem.config.js --only urbanesta-admin-frontend-dev",
    "pm2:stop": "pm2 stop ecosystem.config.js --only urbanesta-admin-frontend",
    "pm2:stop:dev": "pm2 stop ecosystem.config.js --only urbanesta-admin-frontend-dev",
    "pm2:restart": "cp env.production .env && pm2 restart ecosystem.config.js --only urbanesta-admin-frontend",
    "pm2:restart:dev": "cp env.development .env && pm2 restart ecosystem.config.js --only urbanesta-admin-frontend-dev",
    "pm2:delete": "pm2 delete ecosystem.config.js --only urbanesta-admin-frontend",
    "pm2:delete:dev": "pm2 delete ecosystem.config.js --only urbanesta-admin-frontend-dev",
    "pm2:logs": "pm2 logs urbanesta-admin-frontend",
    "pm2:logs:dev": "pm2 logs urbanesta-admin-frontend-dev",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "deploy": "npm run build:prod && pm2 restart urbanesta-admin-frontend",
    "clean": "rm -rf .next out build",
    "logs": "pm2 logs urbanesta-admin-frontend"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4"
  }
}
