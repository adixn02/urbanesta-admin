{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:env": "cp env.development .env",
    "prod:env": "cp env.production .env",
    "start": "cp env.production .env && node index.js",
    "start:dev": "cp env.development .env && node index.js",
    "start:prod": "cp env.production .env && NODE_ENV=production node index.js",
    "dev": "cp env.development .env && nodemon index.js",
    "pm2:start": "cp env.production .env && pm2 start ecosystem.config.js --only urbanesta-admin-api",
    "pm2:start:dev": "cp env.development .env && pm2 start ecosystem.config.js --only urbanesta-admin-api-dev",
    "pm2:stop": "pm2 stop ecosystem.config.js --only urbanesta-admin-api",
    "pm2:stop:dev": "pm2 stop ecosystem.config.js --only urbanesta-admin-api-dev",
    "pm2:restart": "cp env.production .env && pm2 restart ecosystem.config.js --only urbanesta-admin-api",
    "pm2:restart:dev": "cp env.development .env && pm2 restart ecosystem.config.js --only urbanesta-admin-api-dev",
    "pm2:delete": "pm2 delete ecosystem.config.js --only urbanesta-admin-api",
    "pm2:delete:dev": "pm2 delete ecosystem.config.js --only urbanesta-admin-api-dev",
    "pm2:logs": "pm2 logs urbanesta-admin-api",
    "pm2:logs:dev": "pm2 logs urbanesta-admin-api-dev",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "deploy": "cp env.production .env && pm2 restart urbanesta-admin-api",
    "health": "curl -f http://localhost:3002/health || exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@dr.pogodin/csurf": "^1.16.6",
    "@vickos/multer-s3-transforms-v3": "^3.0.1",
    "axios": "^1.13.1",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "multer": "^2.0.2",
    "sharp": "^0.34.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
